<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phw.fitnessclubsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Start"
            android:windowSoftInputMode="adjustPan|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Register"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Administrator"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Johnny_Main_Activity"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Member_Main_Activity"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Member_Course_Info"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Johnny_Course_Info"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Add_Course"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Member_News"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

        <activity android:name=".Johnny_News"
            android:windowSoftInputMode="adjustPan|stateHidden">
        </activity>

    </application>

</manifest>